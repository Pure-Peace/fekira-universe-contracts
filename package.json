{
  "name": "fekira-universe-contracts",
  "version": "0.1.0",
  "description": "FekiraUniverse contracts",
  "engines": {
    "node": ">= 12.18.0"
  },
  "repository": "github:pure-peace/fekira-universe-contracts",
  "author": "pure-peace",
  "license": "MIT",
  "keywords": [
    "ethereum",
    "smart-contracts",
    "template",
    "boilerplate",
    "hardhat",
    "solidity"
  ],
  "devDependencies": {
    "@atixlabs/hardhat-time-n-mine": "0.0.5",
    "@nomiclabs/hardhat-ethers": "^2.0.4",
    "@nomiclabs/hardhat-etherscan": "^3.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/hardhat-upgrades": "^1.12.1",
    "@typechain/ethers-v5": "^8.0.5",
    "@typechain/hardhat": "^3.1.0",
    "@types/chai": "^4.2.21",
    "@types/chai-as-promised": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.7.10",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^7.0.2",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.5.3",
    "fs-extra": "^10.0.0",
    "hardhat": "^2.8.3",
    "hardhat-abi-exporter": "^2.3.0",
    "hardhat-contract-sizer": "^2.4.0",
    "hardhat-deploy": "^0.9.24",
    "hardhat-docgen": "^1.1.2",
    "hardhat-gas-reporter": "^1.0.4",
    "mathjs": "^9.5.0",
    "mocha": "^9.1.1",
    "prettier": "^2.3.2",
    "prettier-plugin-solidity": "^1.0.0-beta.17",
    "solc-0.8.11": "npm:solc@^0.8.11",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.17",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.4.0",
    "typechain": "^6.1.0",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.4.2",
    "@openzeppelin/contracts-upgradeable": "^4.4.2",
    "prompts": "^2.4.2",
    "solidity-docgen": "^0.5.16"
  },
  "scripts": {
    "prepare": "node ./.setup.js && hardhat typechain",
    "lint": "eslint \"**/*.{js,ts}\" && solhint src/**/*.sol",
    "lint:fix": "eslint --fix \"**/*.{js,ts}\" && solhint --fix src/**/*.sol",
    "format": "prettier --check \"**/*.{ts,js,sol}\"",
    "format:fix": "prettier --write \"**/*.{ts,js,sol}\"",
    "compile": "hardhat compile --show-stack-traces --verbose",
    "void:deploy": "hardhat deploy",
    "test": "cross-env HARDHAT_DEPLOY_FIXTURE=true HARDHAT_COMPILE=true mocha --bail --recursive test --timeout 90000",
    "gas": "cross-env REPORT_GAS=true hardhat test",
    "coverage": "cross-env HARDHAT_DEPLOY_FIXTURE=true hardhat coverage",
    "dev": "hardhat node --watch --export contractsInfo.json",
    "local:dev": "hardhat --network localhost deploy --watch",
    "local:hh": "hardhat --network hardhat deploy --watch",
    "execute": "node ./_scripts.js run",
    "export": "node ./_scripts.js export",
    "fork:execute": "node ./_scripts.js fork:run",
    "fork:deploy": "node ./_scripts.js fork:deploy",
    "fork:dev": "node ./_scripts.js fork:dev",
    "fork:node": "node ./_scripts.js fork:node",
    "fork:test": "echo hi && node ./_scripts.js fork:test",
    "docs": "solidity-docgen -i ./src -t ./docs-templates --solc-module solc-0.8.11",
    "deploy-dev": "node ./_scripts.js deploy",
    "deploy-prod": "node ./_scripts.js deploy-prod",
    "size": "hardhat size-contracts"
  }
}
